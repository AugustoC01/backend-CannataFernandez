<div class='div-info'>
  <h1>Datos de la cuenta</h1>
  <table class='profile-table'>
    <tr>
      <td>Email</td>
      <td>{{email}}</td>
    </tr>
    <tr>
      <td>Name</td>
      <td>{{name}}</td>
    </tr>
    <tr>
      <td>Address</td>
      <td>{{address}}</td>
    </tr>
    <tr>
      <td>Edad</td>
      <td>{{age}}</td>
    </tr>
    <tr>
      <td>Telefono</td>
      <td>{{phone}}</td>
    </tr>
    <tr>
      <td><img
          src='/public/avatars/{{avatar}}'
          alt='avatar'
          class='avatar-img'
        /></td>
      <td>{{phone}}</td>
    </tr>
  </table>
</div>